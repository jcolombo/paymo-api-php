{
  "connection": {
    "url": "https://app.paymoapp.com/api/",
    "defaultName": "PaymoApiConnection",
    "verify": false
  },
  "path": {
    "cache": null,
    "logs": null
  },
  "enabled": {
    "cache": false,
    "logging": false
  },
  "devMode": false,
  "classMap": {
    "defaultCollection": "Jcolombo\\PaymoApiPhp\\Entity\\Collection\\EntityCollection",
    "entity": {
      "project": {
        "resource": "Jcolombo\\PaymoApiPhp\\Entity\\Resource\\Project"
      },
      "projects": {
        "resource": "Jcolombo\\PaymoApiPhp\\Entity\\Resource\\Project",
        "collection": true
      },
      "client": {
        "resource": "Jcolombo\\PaymoApiPhp\\Entity\\Resource\\Client"
      },
      "clients": {
        "resource": "Jcolombo\\PaymoApiPhp\\Entity\\Resource\\Client",
        "collection": true
      },
      "projectstatus": {
        "resource": "Jcolombo\\PaymoApiPhp\\Entity\\Resource\\ProjectStatus"
      },
      "projectstatuses": {
        "resource": "Jcolombo\\PaymoApiPhp\\Entity\\Resource\\ProjectStatus",
        "collection": true
      }
    }
  }
}
